																<numIndex index="1">_self</numIndex>
															</numIndex>
															<numIndex index="1" type="array">
																<numIndex index="0">Nouvelle fenêtre</numIndex>
																<numIndex index="1">_blank</numIndex>
															</numIndex>
														</items>
														<default>0</default>
													</config>
												</TCEforms>
											</field_cible>
