									<label>Titre de la section</label>
									<config type="array">
										<type>input</type>
										<size>48</size>
										<eval>trim</eval>
									</config>
								</TCEforms>
							</field_titre_section>
							<field_liens type="array">
								<type>array</type>
								<tx_templavoila type="array">
									<title>liens</title>
									<eType>input</eType>
									<preview></preview>
								</tx_templavoila>
								<el type="array">
									<field_lien type="array">
										<type>array</type>
										<tx_templavoila type="array">
											<title>lien</title>
											<eType>input</eType>
											<TypoScript></TypoScript>
